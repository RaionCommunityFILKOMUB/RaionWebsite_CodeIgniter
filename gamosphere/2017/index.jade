  body
    .loading
      .spinner.spinner-1
    #main_menu.navbar.navbar-default(role='navigation')
      .container-fluid
        .navbar-header
          button.navbar-toggle(data-target='.navbar-menubuilder', data-toggle='collapse', type='button')
            span.sr-only Toggle navigation
            span.icon-bar(id="icon1")
            span.icon-bar(id="icon2")
            span.icon-bar(id="icon3")
          a.navbar-brand
            img(src='assets-gamosphere/img/logo.png')
        .collapse.navbar-collapse.navbar-menubuilder
          ul.nav.navbar-nav
            li
              a(href='#home') HOME
            li
              a(href='#about') ABOUT
            li
              a(href='#speaker') SPEAKER
            li
              a(href='#ticket') TICKET
            li
              a(href='#location') LOCATION
            li
              a(href='#register') REGISTER
            li
              a(href='#contact') CONTACT US

    section.hero  
      .bg-text
        img(src='assets-gamosphere/img/bunderan.png')
      .brand
        .logo
          img(src='assets-gamosphere/img/logo-raion.png')
        .text
          .title SEMINAR 
            strong NASIONAL
          .big GAMOSPHERE
          .sub LET'S BE FRIEND THROUGH THE GAME
          .time 12.11.16
      .satellite.gbr     
        img(src='assets-gamosphere/img/satellite.png')
      .saturn.gbr
        img(src='assets-gamosphere/img/saturn.png')
      .jupiter.gbr
        img(src='assets-gamosphere/img/jupiter.png')
      .earth.gbr
        img(src='assets-gamosphere/img/earth.png')
      .saturn2.gbr
        img(src='assets-gamosphere/img/saturn2.png')
